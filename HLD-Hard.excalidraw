      "type": "text",
      "x": 3060.9792640546393,
      "y": 706.0140894269992,
      "width": 837.5394287109375,
      "height": 350,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
